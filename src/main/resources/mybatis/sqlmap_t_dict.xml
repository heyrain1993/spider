<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="DictMapper">
    <sql id="selectSql">
        SELECT
        `id` as id,
        `first_dict` as firstDict,
        `second_dict` as secondDict,
        `thrid_dict` as thridDict,
        `create_time` as createTime,
        `update_time` as updateTime
        FROM t_dict
    </sql>

    <select id="selectById" resultType="com.heyu.spider.page.entity.dict.Dict">
        <include refid="selectSql" />
        WHERE
        id = #{id}
        LIMIT 1
    </select>


    <insert id="insert" parameterType="com.heyu.spider.page.entity.dict.Dict" useGeneratedKeys="true" keyProperty="id" >
        INSERT INTO
        t_dict (`first_dict`,`second_dict`,`thrid_dict`,`create_time`,`update_time`)
        VALUES (#{firstDict},#{secondDict},#{thridDict},#{createTime},#{updateTime})
    </insert>

    <delete id="deleteById" parameterType="com.heyu.spider.page.entity.dict.Dict">
        DELETE FROM t_dict
        WHERE
        id = #{id}
    </delete>

    <select id="findAllList" resultType="com.heyu.spider.page.entity.dict.Dict">
    <include refid="selectSql" />
    </select>

    <select id="findByCondition" resultType="com.heyu.spider.page.entity.dict.Dict">
    <include refid="selectSql" />
        <where>
            1=1
        </where>
        limit #{offset},#{pageSize}
    </select>

    <select id="getTotalCount" resultType="java.lang.Long">
        SELECT count(1)
        FROM t_dict
        <where>
            1=1
        </where>
    </select>

    <update id="update" parameterType="com.heyu.spider.page.entity.dict.Dict">
        update t_dict
        set
        `first_dict` = #{firstDict},
        `second_dict` = #{secondDict},
        `thrid_dict` = #{thridDict},
        `create_time` = #{createTime},
        `update_time` = #{updateTime}
        where
    id = #{id}
    </update>


</mapper>